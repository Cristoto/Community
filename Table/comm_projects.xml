<table>
    <column>
        <name>idproject</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('comm_projects_idproject_seq'::regclass)</default>
    </column>
    <column>
        <name>name</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>creationdate</name>
        <type>date</type>
    </column>
    <constraint>
        <name>comm_projects_pkey</name>
        <type>PRIMARY KEY (idproject)</type>
    </constraint>
</table>